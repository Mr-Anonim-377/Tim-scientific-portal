{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-plus": "*",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "slider": "^1.0.4",
    "swiper": "^6.3.3",
    "vue": "^3.0.0-0",
    "vue-awesome-swiper": "^4.1.1",
    "vue-axios": "^3.1.3",
    "vue-carousel-3d": "^1.0.1",
    "vue-router": "^4.0.0-beta.13",
    "vue-slick": "^1.1.15",
    "vue-slick-carousel": "^1.0.6",
    "vuex": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0"
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "tabWidth": 4,
          "singleQuote": true
        }
      ],
      "vue/require-default-prop": 0,
      "vue/require-prop-types": 0
    }
  },

  "gitHooks": {
    "pre-commit": "npm run lint"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
