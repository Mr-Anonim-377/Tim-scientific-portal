{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serve-dev": "vue-cli-service serve --mode devEnvironment",
    "serve-prod": "vue-cli-service serve --mode prodEnvironment"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-plus": "*",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "vue": "^3.0.0-0",
    "vue-router": "^4.0.0-beta.13"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "tabWidth": 4,
          "singleQuote": true
        }
      ],
      "vue/require-default-prop": 0,
      "vue/require-prop-types": 0
    }
  },
  "gitHooks": {
    "pre-commit": "npm run lint && git add ."
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
